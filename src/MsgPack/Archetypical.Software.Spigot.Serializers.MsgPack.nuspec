<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Archetypical.Software.Spigot.Serializers.MsgPack</id>
        <version>$version$</version>
        <authors>Archetypical.Software</authors>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>
           A Spigot serializer implementing Message Pack as defined in http://msgpack.org/index.html. 
            This utilizes the nuget packages Newtonsoft.Json and MsgPack.Cli. 
            The code replicates the logic in the package Newtonsoft.Msgpack to be able to handle circular references
        </description>
        <dependencies>
            <dependency id="Archetypical.Software.Spigot" version="1.0.2" />
            <dependency id="MsgPack.Cli" version="1.0.0" />
            <dependency id="Newtonsoft.Json" version="11.0.2" />
        </dependencies>
    </metadata>
    <files>
        <file src="Archetypical.Software.Spigot.Serializers\bin\Release\netstandard2.0\Archetypical.Software.Spigot.Serializers.MsgPack.dll" target="lib\netstandard2.0\Archetypical.Software.Spigot.Serializers.MsgPack.dll" />
    </files>
</package>